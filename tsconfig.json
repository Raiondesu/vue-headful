{
  "compilerOptions": {
    "noEmit": true,
    "target": "es5",
		"moduleResolution": "node",
		"module": "es2015",
    "lib": [
      "dom",
      "es2015",
      "es5"
    ],
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "diagnostics": true,
    "noUnusedParameters": true,
    "sourceMap": true
  }
}
